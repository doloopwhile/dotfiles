// 既定の設定を上書きするには、このファイル内に設定を挿入します
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.un~": true,
        "**/.tmp.*": true
    },
    "editor.fontFamily": "VL Gothic",
    "editor.fontSize": 16,
    "editor.fontWeight": "300",
    "workbench.colorTheme": "Solarized Dark",
    "files.autoSave": "onFocusChange"
}