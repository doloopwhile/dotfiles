#!/usr/bin/env ruby
remove_keys = lambda do |line|
  line.split("\t").map { |s| s.sub(/^[a-zA-Z0-9_\-]*:/, '') }.join("\t")
end

keys = lambda do |line|
  line.split("\t").map { |s| s.sub(/:.*/, '') }.join("\t")
end

first_line = ARGF.gets
puts keys.call(first_line)
puts remove_keys.call(first_line)

while line = ARGF.gets
  puts remove_keys.call(line)
end
